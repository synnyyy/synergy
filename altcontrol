if game.Players.LocalPlayer.Name == Configuration.MainSettings.Host then -- scuffed code im nto bothered to organise it lmao
  content = game.HttpService:JSONEncode(Configuration)
  if isfile("altconfig.txt") then
    delfile("altconfig.txt")
  end
  writefile('altconfig.txt', content)
end

