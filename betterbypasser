
if not cloneref then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/CloneRef.lua",true))()
end

local services = setmetatable({}, {
    __index = function(index, value)
        return cloneref(game:GetService(value))
    end
})

local httprequest = (syn and syn.request) or http and http.request or http_request or (fluxus and fluxus.request) or request
local options = ({...})[1] or {
    Method = 1,
    Keybind = "F",
    ShowMethodDictionary = true
}



do

    local startTime = tick()


    local BetterBypasserNotifThing = Instance.new("ScreenGui")
    local TheFrameItSelf = Instance.new("Frame")
    local WarningTitle = Instance.new("TextLabel")
    local warning = Instance.new("ImageButton")
    local WarningText = Instance.new("TextLabel")
    local DissapearCountdown = Instance.new("TextLabel")
    local Execute = Instance.new("TextButton")
    local UICorner = Instance.new("UICorner")
    local UICorner_2 = Instance.new("UICorner")
    local DiscordJoin = Instance.new("TextButton")
    local UICorner_3 = Instance.new("UICorner")
    
    
    BetterBypasserNotifThing.Name = "BetterBypasser Notif Thing"
    BetterBypasserNotifThing.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    
    TheFrameItSelf.Name = "TheFrameItSelf"
    TheFrameItSelf.Parent = BetterBypasserNotifThing
    TheFrameItSelf.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    TheFrameItSelf.Position = UDim2.new(0.5, -233, 0.5, -174)
    TheFrameItSelf.Size = UDim2.new(0, 466, 0, 347)
    
    WarningTitle.Name = "WarningTitle"
    WarningTitle.Parent = TheFrameItSelf
    WarningTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    WarningTitle.BackgroundTransparency = 1.000
    WarningTitle.Position = UDim2.new(0.241272494, 0, 0.0940850005, 0)
    WarningTitle.Size = UDim2.new(0, 262, 0, 38)
    WarningTitle.Font = Enum.Font.GothamBold
    WarningTitle.Text = "Warning"
    WarningTitle.TextColor3 = Color3.fromRGB(255, 17, 0)
    WarningTitle.TextScaled = true
    WarningTitle.TextSize = 14.000
    WarningTitle.TextStrokeColor3 = Color3.fromRGB(255, 0, 0)
    WarningTitle.TextWrapped = true
    
    warning.Name = "warning"
    warning.Parent = WarningTitle
    warning.BackgroundTransparency = 1.000
    warning.LayoutOrder = 1
    warning.Position = UDim2.new(0.0700687319, 0, 0.0948309526, 0)
    warning.Size = UDim2.new(0, 33, 0, 30)
    warning.ZIndex = 2
    warning.Image = "rbxassetid://3926305904"
    warning.ImageColor3 = Color3.fromRGB(255, 17, 0)
    warning.ImageRectOffset = Vector2.new(364, 324)
    warning.ImageRectSize = Vector2.new(36, 36)
    
    WarningText.Name = "WarningText"
    WarningText.Parent = TheFrameItSelf
    WarningText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    WarningText.BackgroundTransparency = 1.000
    WarningText.Position = UDim2.new(0.0476092547, 0, 0.248839632, 0)
    WarningText.Size = UDim2.new(0, 420, 0, 175)
    WarningText.Font = Enum.Font.Gotham
    WarningText.Text = "There may be bugs with BetterBypasser as it is not functioning properly. We will not provide support for this version. Irregardless, if you would like to execute BetterBypasser, simply click \"Execute\". Otherwise, wait for this UI to go away."
    WarningText.TextColor3 = Color3.fromRGB(255, 255, 255)
    WarningText.TextSize = 22.000
    WarningText.TextStrokeColor3 = Color3.fromRGB(255, 0, 0)
    WarningText.TextWrapped = true
    WarningText.TextYAlignment = Enum.TextYAlignment.Top
    
    DissapearCountdown.Name = "DissapearCountdown"
    DissapearCountdown.Parent = TheFrameItSelf
    DissapearCountdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    DissapearCountdown.BackgroundTransparency = 1.000
    DissapearCountdown.Position = UDim2.new(0.0793755651, 0, 0.875449896, 0)
    DissapearCountdown.Size = UDim2.new(0, 390, 0, 31)
    DissapearCountdown.Font = Enum.Font.Gotham
    DissapearCountdown.Text = "This message will disappear in 15 seconds.."
    DissapearCountdown.TextColor3 = Color3.fromRGB(66, 66, 66)
    DissapearCountdown.TextScaled = true
    DissapearCountdown.TextSize = 22.000
    DissapearCountdown.TextStrokeColor3 = Color3.fromRGB(255, 0, 0)
    DissapearCountdown.TextWrapped = true
    DissapearCountdown.TextYAlignment = Enum.TextYAlignment.Top
    
    Execute.Name = "Execute"
    Execute.Parent = TheFrameItSelf
    Execute.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    Execute.BorderSizePixel = 0
    Execute.Position = UDim2.new(0.0472103134, 0, 0.680115283, 0)
    Execute.Size = UDim2.new(0, 200, 0, 50)
    Execute.Font = Enum.Font.GothamBold
    Execute.Text = "Execute"
    Execute.TextColor3 = Color3.fromRGB(255, 255, 255)
    Execute.TextSize = 14.000
    
    UICorner.CornerRadius = UDim.new(0, 5)
    UICorner.Parent = Execute
    
    UICorner_2.Parent = TheFrameItSelf
    
    DiscordJoin.Name = "DiscordJoin"
    DiscordJoin.Parent = TheFrameItSelf
    DiscordJoin.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    DiscordJoin.BorderSizePixel = 0
    DiscordJoin.Position = UDim2.new(0.521459222, 0, 0.680115283, 0)
    DiscordJoin.Size = UDim2.new(0, 200, 0, 50)
    DiscordJoin.Font = Enum.Font.GothamBold
    DiscordJoin.Text = "Join the Discord"
    DiscordJoin.TextColor3 = Color3.fromRGB(255, 255, 255)
    DiscordJoin.TextSize = 14.000
    
    UICorner_3.CornerRadius = UDim.new(0, 5)
    UICorner_3.Parent = DiscordJoin

    local function joinDiscord()
        httprequest(
            {
                Url = "http://127.0.0.1:6463/rpc?v=1",
                Method = "POST",
                Headers = {
                    ["Content-Type"] = "application/json",
                    ["Origin"] = "https://discord.com"
                },
                Body = services.HttpService:JSONEncode(
                    {
                        cmd = "INVITE_BROWSER",
                        args = {
                            code = "ZvrZHPAwev"
                        },
                        nonce = services.HttpService:GenerateGUID(false)
                    }
                )
            }
        )   
    end

    local function Executee()
        local content = services.HttpService:JSONEncode(options)

        if isfile("data.txt") then
            delfile("data.txt")
        end
        writefile("data.txt", content)

        BetterBypasserNotifThing:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/moresupportedbetterbypasser", true))()
          
    end

    DiscordJoin.MouseButton1Click:Connect(joinDiscord)
    Execute.MouseButton1Click:Connect(Executee)


    local originalPosition = TheFrameItSelf.Position
    local introAnimationDuration = 0.3
    local introAnimationCompleted = false
    local countdownFinished = false
    local countdownStopped = false
    local connection

    local function updateCountdown()
        local elapsedTime = tick() - startTime
        local remainingTime = 15 - elapsedTime
        if remainingTime <= 0 then
            BetterBypasserNotifThing:Destroy()
            countdownFinished = true
        end

        if not countdownStopped then
            if not countdownFinished then
                DissapearCountdown.Text = "This message will disappear in " .. math.ceil(remainingTime) .. " seconds.."
            end

            if not introAnimationCompleted then
                local progress = math.min(elapsedTime / introAnimationDuration, 1)
                local yOffset = 100 * (1 - progress) 
                TheFrameItSelf.Position = originalPosition + UDim2.new(0, 0, 0, yOffset)

                if progress >= 1 then
                    introAnimationCompleted = true
                end
            end
        else
            TheFrameItSelf.Position = originalPosition
        end
    end

    connection = services.RunService.RenderStepped:Connect(updateCountdown)

    local function stopCountdown()
        countdownStopped = true
        connection:Disconnect()
    end

    BetterBypasserNotifThing.AncestryChanged:Connect(function(_, parent)
        if parent == nil then
            stopCountdown()
        end
    end)
end
