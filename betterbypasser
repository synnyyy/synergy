local options = ({...})[1] or { Method = 1, Keybind = "F", ShowMethodDictionary = true};
content = game.HttpService:JSONEncode(options)
if isfile("data.txt") then
    delfile("data.txt")
end
writefile('data.txt', content)

local executor = identifyexecutor() or is_sirhurt_closure and "Sirhurt" or pebc_execute and "ProtoSmasher" or syn and "Synapse X" or secure_load and "Sentinel" or KRNL_LOADED and "Krnl" or SONA_LOADED and "Sona" or "Unknown"
print("[1/2] BetterBypasser | Checking executor")

if executor == "Synapse X" or string.find(executor, "Ware") or executor == "Krnl" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/normalbetterbypasser",true))()
elseif executor == "Unknown" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/moresupportedbetterbypasser",true))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/moresupportedbetterbypasser",true))()
end

print("[2/2] BetterBypasser | Launched application successfully")

