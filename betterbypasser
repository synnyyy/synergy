repeat wait() until game:IsLoaded()
local Services = setmetatable({}, {
    __index = function(_, serviceName)
        local service = cache[serviceName]
        if not service then
            service = cloneref(game:GetService(serviceName))
        end
        return service
    end
})


local starterGui = Services.StarterGui
setclipboard("https://discord.gg/QXq2KTVe4j")
starterGui:SetCore("SendNotification", {
    Title = "On Maintenance",
    Text = "BetterBypasser is on maintenance. Check back on the status of this script on discord.gg/QXq2KTVe4j. The link to the Discord Server has been pasted on your clipboard.",
    Duration = 5 -- Duration in seconds
})
