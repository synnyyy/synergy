if not cloneref then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/CloneRef.lua",true))()
end

local services = setmetatable({}, {
    __index = function(index, value)
        return cloneref(game:GetService(value))
    end
})

local options = ({...})[1] or {
    Method = 1,
    Keybind = "F",
    ShowMethodDictionary = true
}

 local function Executee()
    local content = services.HttpService:JSONEncode(options)

    if isfile("data.txt") then
        delfile("data.txt")
    end
    writefile("data.txt", content)

    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/normalbetterbypasser", true))()
          
end
