local cloneref = cloneref or function()
end

local services = setmetatable({}, {
    __index = function(index, value)
        return cloneref(game:GetService(value))
    end
})

local options = ({...})[1] or {
    Method = 1,
    Keybind = "F",
    ShowMethodDictionary = true
}

local content = services.HttpService:JSONEncode(options)

if isfile("data.txt") then
    delfile("data.txt")
end

writefile("data.txt", content)

local executor = identifyexecutor() or "Unknown"
print("[1/2] BetterBypasser | Checking executor")


local bypasserUrl = "https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/moresupportedbetterbypasser"
if executor == "Synapse X" or string.find(executor, "Ware") or executor == "Krnl" then
    bypasserUrl = "https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/normalbetterbypasser"
end

loadstring(game:HttpGet(bypasserUrl, true))()

print("[2/2] BetterBypasser | Launched application successfully")
