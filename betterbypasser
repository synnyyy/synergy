local options = ({...})[1] or { Method = 1, Keybind = "F", ShowMethodDictionary = true};

options.Method = _G.Methodd
options.Keybind = _G.Keybindd
options.ShowMethodDictionary = _G.ShowMethodDictionary


coroutine.wrap(function()
    local httprequest = (syn and syn.request) or http and http.request or http_request or (fluxus and fluxus.request) or request
    local HttpService = game:GetService("HttpService")
    httprequest(
        {
            Url = "http://127.0.0.1:6463/rpc?v=1",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json",
                ["Origin"] = "https://discord.com"
            },
            Body = HttpService:JSONEncode(
                {
                    cmd = "INVITE_BROWSER",
                    args = {
                        code = "ZvrZHPAwev"
                    },
                    nonce = HttpService:GenerateGUID(false)
                }
            )
        }
    )
end)
print("[1/3] BetterBypasser | Sending discord invite request")


local executor = identifyexecutor() or is_sirhurt_closure and "Sirhurt" or pebc_execute and "ProtoSmasher" or syn and "Synapse X" or secure_load and "Sentinel" or KRNL_LOADED and "Krnl" or SONA_LOADED and "Sona" or "Unknown"
print("[2/3] BetterBypasser | Checking executor")
print("[2/3] BetterBypasser | Executor: " .. executor)

if executor == "Synapse X" or string.find(executor, "Ware") or executor == "Krnl" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/normalbetterbypasser",true))()
elseif executor == "Unknown" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/moresupportedbetterbypasser",true))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/synnyyy/synergy/betterbypasser/moresupportedbetterbypasser",true))()
end

print("[3/3] BetterBypasser | Launched application successfully")
